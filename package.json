{
	"name": "gears_and_springs",
	"version": "0.0.1",
	"description": "parameters and stl for gears and springs",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/gears_and_springs.git"
	},
	"homepage": "https://github.com/charlyoleg2/gears_and_springs#readme",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"designix",
		"parametrix"
	],
	"type": "module",
	"main": "index.js",
	"scripts": {
		"designix-uis": "designix-uis",
		"designix-cli": "designix-cli",
		"designix-cli-help": "designix-cli --help",
		"gearWW_list-outopt": "designix-cli --design desi82/gearWW list-outopt",
		"gearWW_json_param": "designix-cli --design=desi82/gearWW write json_param",
		"gearWW_openscad_long": "designix-cli --design=desi82/gearWW --param=refs/gearWW/px_gearWW_v01.json --outDir=refs/gearWW --outFileName=gearWW_v01.scad write scad_3d_openscad",
		"gearWW_openscad": "designix-cli -d=desi82/gearWW -p=refs/gearWW/px_gearWW_v01.json -o=refs/gearWW --outFileName=gearWW_v01.scad write scad_3d_openscad",
		"gearWW_view": "openscad refs/gearWW/gearWW_v01.scad",
		"gearWW_stl": "openscad -o refs/gearWW/gearWW_v01.stl refs/gearWW/gearWW_v01.scad",
		"make_gear": "node make_gear.js",
		"git_clean": "git clean -f",
		"ls_output": "ls -ltra output"
	},
	"devDependencies": {
		"designix-cli": "^0.5.27",
		"designix-uis": "^0.5.17"
	}
}
